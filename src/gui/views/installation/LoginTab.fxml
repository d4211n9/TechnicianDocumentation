<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<VBox fx:id="loginTab" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="100.0" minWidth="150.0" prefHeight="444.0" prefWidth="840.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.controllers.installation.LoginTabController">
   <children>
      <TableView fx:id="tableView" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="250.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
         <columns>
            <TableColumn fx:id="tcDevice" prefWidth="250.0" text="Device" />
            <TableColumn fx:id="tcUsername" prefWidth="250.0" text="Username" />
            <TableColumn fx:id="tcPassword" minWidth="0.0" prefWidth="250.0" text="Password" />
            <TableColumn fx:id="tcID" prefWidth="60.0" text="ID" />
         </columns>
      </TableView>
      <HBox fx:id="hbEditArea" alignment="CENTER" disable="true" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="60.0" prefWidth="200.0" spacing="10.0" VBox.vgrow="NEVER">
         <children>
            <Label text="Update login information:">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <Label fx:id="lblSelectedDevice" text="Select a device...">
               <font>
                  <Font size="14.0" />
               </font>
            </Label>
            <TextField fx:id="txtUsername" promptText="Username">
               <font>
                  <Font size="14.0" />
               </font>
            </TextField>
            <TextField fx:id="txtPassword" promptText="Password">
               <font>
                  <Font size="14.0" />
               </font>
            </TextField>
         </children>
      </HBox>
      <HBox fx:id="loginBtnArea" alignment="CENTER" maxHeight="-Infinity" maxWidth="1.7976931348623157E308" minHeight="-Infinity" prefHeight="60.0" prefWidth="820.0" />
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
